#include <iostream>
using namespace std;

int main() {
    int cupCount = 0;
    cout << "How many cups? ";
    cin >> cupCount;

    double totalCost = cupCount * 1.0;  

    if (cupCount >= 10) {
        totalCost *= 0.8; 
    } else if (cupCount >= 5) {
        totalCost *= 0.9; 
    } else {
        totalCost *= 1;   
    }

    cout << "Total cost: $" << totalCost << endl;

    return 0;
}
