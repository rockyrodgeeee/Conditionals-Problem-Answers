#include <iostream>
using namespace std;

int main() {
    int numberOfCups = 0;
    cout << "How many cups? ";
    cin >> numberOfCups;

    double totalPrice = numberOfCups * 1.0;  

    if (numberOfCups >= 10) {
        totalPrice *= 0.8; 
    } else if (numberOfCups >= 5) {
        totalPrice *= 0.9; 
    } else {
        totalPrice *= 1;   
    }

    cout << "Total cost: $" << totalPrice << endl;

    return 0;
}
